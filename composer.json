{
    "name": "jubeki/orbit-git",
    "description": "Provide git features for the flat-file eloquent driver orbit",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Jubeki/orbit-git",
    "authors": [
        {
            "name": "Julius Kiekbusch",
            "email": "contact@julius-kiekbusch.de",
            "homepage": "https://julius-kiekbusch.de",
            "role": "Developer"
        }
    ],
    "keywords": [
        "laravel",
        "orbit",
        "git"
    ],
    "autoload": {
        "psr-4": {
            "Jubeki\\OrbitGit\\": "src"
        }
    },
    "require": {
        "php": "^8.0",
        "ryangjchandler/git": "^0.0.4",
        "ryangjchandler/orbit": "^1.1.2"
    },
    "require-dev": {
        "jubeki/laravel-code-style": "^1.0.0",
        "nunomaduro/larastan": "^2.1",
        "orchestra/testbench": "^7.1",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jubeki\\OrbitGit\\OrbitGitServiceProvider"
            ]
        }
    },
    "scripts": {
        "check-style": "php-cs-fixer fix --dry-run --diff",
        "fix-style": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
